{
    "comments": {
        "lineComment": "//",
        "blockComment": [
            "/*",
            "*/"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string",
                "comment"
            ]
        },
        {
            "open": "'",
            "close": "'",
            "notIn": [
                "string",
                "comment"
            ]
        },
        {
            "open": "/*",
            "close": "*/",
            "notIn": [
                "string"
            ]
        }
    ],
    "surroundingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "'",
            "close": "'"
        }
    ],
    "folding": {
        "markers": {
            "start": "^\\s*/\\*|^\\s*\\{",
            "end": "^\\s*\\*/|^\\s*\\}"
        }
    },
    "indentationRules": {
        "increaseIndentPattern": "^.*\\{\\s*$",
        "decreaseIndentPattern": "^\\s*\\}"
    },
    "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}
